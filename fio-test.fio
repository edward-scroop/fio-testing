[global]
runtime=120
ramp_time=5
ioengine=io_uring
#numjobs=$ncpus
numjobs=16
group_reporting
direct=1
size=10GiB

[4k-sw-qd1]
stonewall
bs=4k
rw=write
iodepth=1

[4k-sw-qd32]
stonewall
bs=4k
rw=write
iodepth=32

[4k-sr-qd1]
stonewall
bs=4k
rw=read
iodepth=1

[4k-sr-qd32]
stonewall
bs=4k
rw=read
iodepth=32

[4k-rw-qd1]
stonewall
bs=4k
rw=randwrite
iodepth=1

[4k-rw-qd32]
stonewall
bs=4k
rw=randwrite
iodepth=32

[4k-rr-qd1]
stonewall
bs=4k
rw=randread
iodepth=1

[4k-rr-qd32]
stonewall
bs=4k
rw=randread
iodepth=32

[4k-70rr30rw-qd1]
stonewall
bs=4k
rw=randrw
rwmixwrite=30
iodepth=1

[4k-70rr30rw-qd32]
stonewall
bs=4k
rw=randrw
rwmixwrite=30
iodepth=32

[4k-70sr30sw-qd1]
stonewall
bs=4k
rw=rw
rwmixwrite=30
iodepth=1

[4k-70sr30sw-qd32]
stonewall
bs=4k
rw=rw
rwmixwrite=30
iodepth=32
